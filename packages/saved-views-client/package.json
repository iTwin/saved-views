{
  "name": "@itwin/saved-views-client",
  "version": "0.5.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/saved-views.git",
    "directory": "packages/saved-views-client"
  },
  "homepage": "https://github.com/iTwin/saved-views/tree/master/packages/saved-views-client",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "https://www.bentley.com"
  },
  "type": "module",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project ./tsconfig.esm.json",
    "build:cjs": "tsc --project ./tsconfig.cjs.json && node ../../scripts/cjs/postBuild.mjs ./lib/cjs",
    "clean": "rm -rf lib",
    "test": "vitest run",
    "test:cover": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "happy-dom": "^15.11.4",
    "typescript": "^5.5.4",
    "vite": "^6.0.0",
    "vitest": "^3.2.4"
  }
}
