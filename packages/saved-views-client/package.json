{
  "name": "@itwin/saved-views-client",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/saved-views-react.git",
    "directory": "packages/saved-views-client"
  },
  "homepage": "https://github.com/iTwin/saved-views-react/pakages/saved-views-client",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "https://www.bentley.com"
  },
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "./lib",
    "./public"
  ],
  "scripts": {
    "build": "run-p build:**",
    "build:copy-assets:esm": "cpx \"./src/**/*.{scss,css}\" ./lib/esm",
    "build:copy-assets:cjs": "cpx \"./src/**/*.{scss,css}\" ./lib/cjs",
    "build:transpile:esm": "tsc --project ./tsconfig.build.json",
    "build:transpile:cjs": "tsc --project ./tsconfig.build.json --module CommonJS --moduleResolution Node16 --outDir ./lib/cjs",
    "test": "vitest",
    "test:cover": "vitest run --coverage",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.+(ts|tsx|js|jsx)\"",
    "lint-and-fix": "eslint \"src/**/*.+(ts|tsx|js|jsx)\" --fix"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "cpx2": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "base64-arraybuffer": "^1.0.2",
    "class-validator": "^0.14.0",
    "uuid": "^9.0.0"
  }
}
