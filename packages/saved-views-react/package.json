{
  "name": "@itwin/saved-views-react",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/saved-views-react.git",
    "directory": "packages/saved-views-react"
  },
  "homepage": "https://github.com/iTwin/saved-views-react",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "https://www.bentley.com"
  },
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "./lib",
    "./public"
  ],
  "scripts": {
    "build": "run-p build:esm build:cjs",
    "build:esm": "run-s build:transpile:esm build:css:esm",
    "build:cjs": "run-s build:transpile:cjs build:css:cjs",
    "build:transpile:esm": "tsc --project ./tsconfig.build.json",
    "build:transpile:cjs": "tsc --project ./tsconfig.build.json --module CommonJS --moduleResolution Node16 --outDir ./lib/cjs",
    "build:css:esm": "postcss src/**/*.css --base ./src --dir ./lib/esm",
    "build:css:cjs": "postcss src/**/*.css --base ./src --dir ./lib/cjs",
    "test": "vitest run --passWithNoTests",
    "test:cover": "vitest run --coverage --passWithNoTests",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@itwin/appui-abstract": "^4.0.6",
    "@itwin/appui-react": "^4.3.0",
    "@itwin/core-bentley": "^4.0.6",
    "@itwin/core-common": "^4.0.6",
    "@itwin/core-frontend": "^4.0.6",
    "@itwin/core-geometry": "^4.0.6",
    "@itwin/core-i18n": "^4.0.6",
    "@itwin/core-react": "^4.3.0",
    "@testing-library/react": "^12.1.5",
    "@types/lodash": "^4.14.196",
    "@types/react": "^17.0.39",
    "@types/react-redux": "^7.1.25",
    "@types/react-select": "^3.0.26",
    "@types/react-window": "^1.8.5",
    "@types/recursive-readdir": "^2.2.2",
    "@types/uuid": "^9.0.2",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "cpx2": "^5.0.0",
    "happy-dom": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "postcss-modules": "^6.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux-typescript": "3.0.0-rc.3",
    "recursive-readdir": "^2.2.3",
    "typescript": "^5.1.6",
    "vite": "^4.4.11",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "@itwin/appui-abstract": "^4.0.6",
    "@itwin/appui-layout-react": "^4.0.6",
    "@itwin/appui-react": "^4.3.0",
    "@itwin/core-bentley": "^4.0.6",
    "@itwin/core-common": "^4.0.6",
    "@itwin/core-frontend": "^4.0.6",
    "@itwin/core-geometry": "^4.0.6",
    "@itwin/core-i18n": "^4.0.6",
    "@itwin/core-react": "^4.3.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "@itwin/imodel-components-react": "^4.3.0",
    "@itwin/itwinui-icons-react": "^2.4.0",
    "@itwin/itwinui-react": "^2.12.10",
    "@itwin/saved-views-client": "workspace:*",
    "@reduxjs/toolkit": "^1.9.5",
    "base64-arraybuffer": "^1.0.2",
    "class-validator": "^0.14.0",
    "fuse.js": "^6.6.2",
    "lodash": "^4.17.21",
    "matcher": "^4.0.0",
    "react-redux": "^7.2.2",
    "react-select": "^3.1.0",
    "react-spring": "^8.0.27",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9",
    "redux": "^4.1.0",
    "uuid": "^9.0.0"
  }
}
