{
  "name": "@itwin/saved-views-react",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/iTwin/saved-views-react.git",
    "directory": "packages/saved-views-react"
  },
  "homepage": "https://github.com/iTwin/saved-views-react",
  "author": {
    "name": "Bentley Systems, Inc.",
    "url": "https://www.bentley.com"
  },
  "types": "./lib/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/index.d.ts",
        "default": "./lib/esm/index.js"
      },
      "require": {
        "types": "./lib/cjs/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "files": [
    "./lib",
    "./public"
  ],
  "scripts": {
    "build": "run-p build:**",
    "build:copy-assets:esm": "cpx \"./src/**/*.{scss,css}\" ./lib/esm",
    "build:copy-assets:cjs": "cpx \"./src/**/*.{scss,css}\" ./lib/cjs",
    "build:transpile:esm": "tsc --project ./tsconfig.build.json",
    "build:transpile:cjs": "tsc --project ./tsconfig.build.json --module CommonJS --moduleResolution Node16 --outDir ./lib/cjs",
    "test": "vitest run --passWithNoTests",
    "test:cover": "vitest run --coverage --passWithNoTests",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/react": "^17.0.39",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "@vitest/coverage-v8": "^0.33.0",
    "cpx2": "^5.0.0",
    "happy-dom": "^10.5.2",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-redux": "^7.2.2",
    "redux": "^4.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  }
}
